<?xml version="1.0"?>
<package >
  <metadata>
    <id>Cronos</id>
    <version>%version%</version>
    <title>Cronos</title>
    <authors>Andrey Dorokhov, Sergey Odinokov</authors>
    <owners>HangfireIO, odinserj</owners>
    <projectUrl>https://github.com/HangfireIO/Cronos</projectUrl>
